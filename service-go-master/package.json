    "express-flash-messages": "^0.1.1",
    "express-handlebars": "^3.1.0",
    "express-jwt": "^5.3.1",
    "express-messages": "^1.0.1",
    "express-session": "^1.16.2",
    "express-validator": "^6.1.1",
    "fs": "0.0.1-security",
    "handlebars": "^4.3.4",
    "hbs": "^4.0.5",
    "i": "^0.3.6",
    "jsonwebtoken": "^8.5.1",
    "lodash.merge": "^4.6.2",
    "moment": "^2.24.0",
