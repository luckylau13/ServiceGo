{
  "name": "service-go",
  "version": "1.0.0",
  "description": "The web-based application for household works.",
  "main": "index.js",
  "directories": {
    "test": "mocha"
  },
  "scripts": {
    "clean": "rm -rf build && mkdir build",
    "build-babel": "babel -d ./build ./src -s",
    "build": "npm run clean && npm run build-babel && cp -r ./src/views ./build && cp -r ./src/data ./build && cp -r ./src/public ./build",
    "start": "npm run build && node ./build/index.js",
    "dev": "nodemon --exec babel-node ./src/index.js",
    "test:unit": "./node_modules/.bin/mocha --require @babel/register test/unit/**/*.js --timeout 10000 --exit",
    "test:acceptance": "./node_modules/.bin/nightwatch",
    "test": "npm run test:unit && npm run test:acceptance"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sharad-ghimire/service-go.git"
  },
  "author": "Sharad Ghimire",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sharad-ghimire/service-go/issues"
  },
  "homepage": "https://github.com/sharad-ghimire/service-go#readme",
  "dependencies": {
    "@google/maps": "^0.5.5",
    "@sendgrid/mail": "^6.4.0",
    "applicationinsights": "^1.4.1",
    "bcrypt": "^3.0.6",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "chai-http": "^4.3.0",
    "connect-flash": "^0.1.1",
